{
	"servers": {
		"sonarqube": {
			"command": "docker",
			"args": [
				"run",
				"-i",
				"--rm",
				"-e",
				"SONARQUBE_TOKEN",
				"-e",
				"SONARQUBE_ORG",
				"mcp/sonarqube"
			],
			"env": {
				"SONARQUBE_TOKEN": "${input:SONARQUBE_TOKEN}",
				"SONARQUBE_ORG": "${input:SONARQUBE_ORG}"
			},
			"type": "stdio"
		}
	},
	"inputs": [
		{
			"id": "SONARQUBE_TOKEN",
			"type": "promptString",
			"description": "SonarQube Cloud Token",
			"password": true
		},
		{
			"id": "SONARQUBE_ORG",
			"type": "promptString",
			"description": "SonarQube Cloud Organization Key",
			"password": false
		}
	]
}